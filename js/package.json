{
  "name": "xoq",
  "version": "0.3.4",
  "description": "MoQ-based serial port streaming for the web",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "examples": "vite examples",
    "build:pages": "vite build --config vite.config.js && cp -r examples/urdf examples/assets dist-pages/ && rm -f dist-pages/assets/meshes/.gitignore",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "moq",
    "serial",
    "webserial",
    "webtransport",
    "streaming"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/w3c-web-serial": "^1.0.8",
    "playwright": "^1.58.2",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@moq/lite": "^0.1.0",
    "three": "^0.182.0",
    "urdf-loader": "^0.12.6"
  }
}
